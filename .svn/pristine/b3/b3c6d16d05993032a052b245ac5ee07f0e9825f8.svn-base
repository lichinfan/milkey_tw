package com.kentec.milkbox.coupon.data;

public class Coupon {
	
	private String id;	
	private String name;
	private String image;
	private String thumbnail;
	private String shortDesc;
	private String desc;
	private String expDate;
	private String code;
	private String linkSku;
	private String linkStoreId;
	private int status;
	
	public static final int USE = 1;		
	
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getImage() {
		return image;
	}
	public void setImage(String image) {
		this.image = image;
	}	
	public String getThumbnail() {
		return thumbnail;
	}
	public void setThumbnail(String thumbnail) {
		this.thumbnail = thumbnail;
	}
	public String getShortDesc() {
		return shortDesc;
	}
	public void setShortDesc(String shortDesc) {
		this.shortDesc = shortDesc;
	}
	public String getDesc() {
		return desc;
	}
	public void setDesc(String desc) {
		this.desc = desc;
	}
	public String getExpDate() {
		return expDate;
	}
	public void setExpDate(String expDate) {
		this.expDate = expDate;
	}
	public int getStatus() {
		return status;
	}
	public void setStatus(int status) {
		this.status = status;
	}
	public String getCode() {
		return code;
	}
	public void setCode(String code) {
		this.code = code;
	}
	public String getLinkSku() {
		return linkSku;
	}
	public void setLinkSku(String linkSku) {
		this.linkSku = linkSku;
	}
	public String getLinkStoreId() {
		return linkStoreId;
	}
	public void setLinkStoreId(String linkStoreId) {
		this.linkStoreId = linkStoreId;
	}
	
}
